=20
        fun getInstance(context: Context): ProductRoomDatabase {
            synchronized(this) {
                var instance =3D INSTANCE
=20
                if (instance =3D=3D null) {
                    instance =3D Room.databaseBuilder(
                        context.applicationContext,
                        ProductRoomDatabase::class.java,
                        "product_database"
                    ).fallbackToDestructiveMigration()
                        .build()
=20
                    INSTANCE =3D instance
                }
                return instance
            }
